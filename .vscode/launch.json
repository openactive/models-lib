{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Generate TypeScript",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "src/app.js",
      "args": [
        "generate", "TypeScript", "--destination", "./tmp/src"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Generate .NET",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "src/app.js",
      "args": [
        "generate", ".NET", "--destination", "../OpenActive.NET/OpenActive.NET/"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Generate PHP",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "src/app.js",
      "args": [
        "generate", "PHP", "--destination", "../models-php/src/"
      ]
    }
  ]
}